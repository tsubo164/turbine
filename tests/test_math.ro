[math]
[test1]

# main() int

  ---
    test1.AssertF(3.141592653589793, math._PI_)
    test1.AssertF(2.718281828459045, math._E_)
 
  ---
    - f = 25.0
    test1.AssertF(5.0, math.sqrt(f))
    - x = 16.0
    test1.AssertF(4.0, math.sqrt(x))
    x = 9.0
    test1.AssertF(3.0, math.sqrt(x))

  /*
  ---
    test1.AssertF(81.0, math.pow(x, 2))
 
  ---
      - r = math.sin(math._PI_ / 2)
      test1.AssertF(1.0, r)
  ---
      - x = 45.0
      test1.AssertF(0.7071067811865476, math.sin(math.radians(x)))
      test1.AssertF(0.0, math.cos(math._PI_ / 2))
      test1.AssertF(-1.0, math.cos(math._PI_))

  ---
      - x = 2.0
      - y = 3.0
      test1.AssertF(8.0, math.pow(x, y))

  ---
      - f1 = 3.0
      - f2 = 4.0
      test1.AssertF(5.0, math.sqrt(math.pow(f1, 2) + math.pow(f2, 2)))

      - r1 = 1.0
      - r2 = 1.0
      test1.AssertF(math._PI_, math.area_circle(r1))
      test1.AssertF(4 * math._PI_, math.area_circle(2 * r2))
  */


  ---
    - v = math.Vec3 { x = 1.1, y = 2.2, z = 3.3 }
    test1.AssertF(1.1, v.x)
    test1.AssertF(2.2, v.y)
    test1.AssertF(3.3, v.z)

    - a = math.Vec3 { x = 4.4, y = 5.5, z = 6.6 }
    test1.AssertF(4.4, a.x)
    test1.AssertF(5.5, a.y)
    test1.AssertF(6.6, a.z)
 
    - b = math.Vec3 { x = -1.0, y = 0.0, z = 1.0 }
    test1.AssertF(-1.0, b.x)
    test1.AssertF(0.0, b.y)
    test1.AssertF(1.0, b.z)

    - c = math.Vec3 { x = 0.0, y = -2.5, z = 2.5 }
    test1.AssertF(0.0, c.x)
    test1.AssertF(-2.5, c.y)
    test1.AssertF(2.5, c.z)

  print(test1._test_count_, "tests done.")

  return 0
