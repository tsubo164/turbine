  ---
    // "break" statment
    - i int
    for j in 0..10
      if j == 5
        break
      i++
    test1.AssertI(5, i)

  ---
    // "continue" statment
    - j int
    j = 0
    for i in 0..10
      if i % 2 == 0
        continue
      j++
    test1.AssertI(5, j)

  // for zero times loop
  ---
    - a = 13
    for i in 0..0
      a *= 2
    test1.AssertI(13, a)

